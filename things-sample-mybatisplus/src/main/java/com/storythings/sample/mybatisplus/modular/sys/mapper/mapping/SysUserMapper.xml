<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.storythings.sample.mybatisplus.modular.sys.mapper.SysUserMapper">

    <select id="fetchUsersByXml" resultType="com.storythings.sample.mybatisplus.modular.sys.entity.SysUser">
        select
            *
        from things_sys_user
        where 1 = 1
    </select>

</mapper>